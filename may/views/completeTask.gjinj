<query>
mutation completeTask($input: CompleteTaskMutationInput!){
  taskComplete(input: $input){
    id
      errors{
      messages
      }
  }
}
</query>

<display>
{% if not errors %}
{% set task = data.taskComplete %}
Succesfully completed {{task.id}}!
{{task.errors}}
{% else %}
Error completing tasks: {{ errors }}
{% endif %}
</display>
